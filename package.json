{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "A starter-kit for react",
  "main": "src/app.js",
  "scripts": {
    "watch:js": "NODE_ENV=development watchify src/app.js -t babelify -p livereactload -o public/scripts/bundle.js -v --debug",
    "build": "NODE_ENV=production browserify src/app.js -t babelify -o public/scripts/bundle.js -v",
    "serve": "node index.js & npm run watch:js",
    "test": "jest --coverage"
  },
  "keywords": [
    "react",
    "starter",
    "hot-reloading"
  ],
  "author": "Harish Yeluri",
  "license": "ISC",
  "devDependencies": {
    "babel-jest": "^9.0.3",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "jest-cli": "^0.9.2",
    "livereactload": "^2.2.1",
    "react-proxy": "^1.1.8",
    "serve": "^1.4.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "browserify": "^13.0.0",
    "express": "^4.13.4",
    "immutable": "^3.7.6",
    "radium": "^0.16.6",
    "react": "^15.0.0-rc.2",
    "react-bootstrap": "^0.28.4",
    "react-dom": "^15.0.0-rc.2",
    "react-router": "^2.0.1",
    "watchify": "^3.7.0"
  }
}
